    <Panels:MainPanel x:Name="mainPanel" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="1300"/>
    <Panels:ExercisePanel x:Name="exercisePanel" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="1300" Visibility="Hidden"/>
    <Panels:TestPanel x:Name="testPanel" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="1300" Visibility="Hidden"/>
